{
  "name": "boilerplate-typescript-monorepo",
  "version": "0.0.1",
  "description": "Development template for NodeJS(18.11.x) with Typescript4.9.x in VSCode",
  "scripts": {
    "dev": "turbo dev",
    "test": "turbo test",
    "build": "turbo build",
    "packaging": "turbo packaging",
    "postinstall": "manypkg check",
    "kill_node_modules": "pnpm rimraf ./**/node_modules",
    "reset_modules": "rm -rf ./pnpm-lock.yaml && pnpm store prune && pnpm kill_node_modules && rm -rf ~/Library/pnpm/store/v3/files/"
  },
  "keywords": [
    "boilerplate",
    "typescript",
    "monorepo"
  ],
  "author": {
    "name": "Bob",
    "email": "baeky.dev@gmail.com"
  },
  "private": true,
  "license": "ISC",
  "packageManager": "pnpm@7.17.0^",
  "dependencies": {
    "@manypkg/cli": "^0.19.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "29.2.5",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "jest-junit": "^15.0.0",
    "pkg": "^5.8.0",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "turbo": "^1.7.0",
    "typescript": "4.9.4"
  }
}
